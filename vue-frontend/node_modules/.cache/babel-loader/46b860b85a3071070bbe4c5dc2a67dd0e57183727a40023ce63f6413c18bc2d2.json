del=\"nuevaOrden.chofer.documento\" />\n\n        <label>Nombre del chofer:</label>\n        <input type=\"text\" v-model=\"nuevaOrden.chofer.nombre\" />\n\n        ...\n\n        <label>Email para Alerta:</label>\n        <input type=\"text\" v-model=\"nuevaOrden.alerta.mail\" />\n\n        <label>Temperatura Max:</label>\n        <input type=\"text\" v-model=\"nuevaOrden.alerta.temperaturaMax\" />\n\n        ...\n\n        <label>Nombre del producto:</label>\n        <input type=\"text\" v-model=\"nuevaOrden.producto.nombre\" />\n\n        <label>Descripcion del producto:</label>\n        <input type=\"text\" v-model=\"nuevaOrden.producto.descripcion\" />\n\n        ...\n\n        <label>Codigo externo:</label>\n        <input type=\"text\" v-model=\"nuevaOrden.codigoExterno\" />\n\n\n        <button @click=\"createOrden\">Crear orden</button>\n    </div>\n</template>\n\n<script>\nimport OrdenService from '@/services/OrdenService';\n\nexport default {\n    name: '',\n    data() {\n        return {\n            token: '',\n            requestCompleteCrearOrdenes: false,\n            nuevaOrden: {\n                \"fechaRecepcion\": \"2022-11-18\",\n                \"cliente\": {\n                    \"razonSocial\": \"razonSocial\",\n                    \"contacto\": 123451234\n                },\n                \"camion\": {\n                    \"cisternado\": [123, 423, 342],\n                    \"descipcion\": \"asdfga\",\n                    \"patente\": \"asfasd2313asdasd\"\n                },\n                \"chofer\": {\n                    \"apellido\": \"apellido\",\n                    \"documento\": 332312313,\n                    \"nombre\": \"nombre\"\n                },\n                \"alerta\": {\n                    \"mail\": \"[ayecano98@gmail.com]\",\n                    \"temperaturaMax\": 50\n                },\n                \"producto\": {\n                    \"descipcion\": \"asdasd\",\n                    \"nombre\": \"asdasd\"\n                },\n                \"codigoExterno\": 242\n            }\n\n\n        }\n    },\n    methods: {\n        async createOrden() {\n            try {\n                // Obtener un token llamando al método \"getTokens\" importado\n\n                // Llamar al método \"createOrden\" del servicio de orden, pasando la nueva orden y el token como parámetros\n                this.nuevaOrden = await OrdenService.createOrden(this.nuevaOrden, this.token);\n                console.log('Orden creada correctamente:', nuevaOrden);\n                this.requestCompleteCrearOrdenes = true;\n            } catch (error) {\n                console.error(error);\n            }\n        }\n    },\n    // created(){\n    //      this.getCamiones()\n    // }\n\n}\n\n</script>\n\n<style>\n.container {\n    margin: 0 auto;\n}\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}